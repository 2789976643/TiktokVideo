
.tab{
	width: 100%;
	height: 50px;
	line-height: 50px;
	bottom: 0px;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	text-align: center;
}
.tab-box {
	width: 100%;
	background: #000;
	color: white;
}
.icon-box {
	width: 60%;
	height: 25px;
	line-height: 25px;
	margin: 12.5px 20%;
	background: white;
	color: #000000;
	border-radius: 4px;
	font-size: 15px;
}

